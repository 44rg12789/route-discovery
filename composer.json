{
    "name": "laravel/route-discovery",
    "description": "Auto register routes using PHP attributes.",
    "keywords": [
        "laravel-route-discovery"
    ],
    "homepage": "https://github.com/laravel/route-discovery",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.67|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.77|^9.0|^10.0|^11.0",
        "symfony/finder": "^5.4.2|^6.0|^7.0"
    },
    "require-dev": {
        "nunomaduro/larastan": "^1.0.2|^2.0",
        "orchestra/testbench": "^6.23.2|^7.0|^8.0",
        "pestphp/pest": "^1.21",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-package-tools": "^1.10",
        "spatie/laravel-ray": "^1.27"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\RouteDiscovery\\": "src",
            "Laravel\\RouteDiscovery\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\RouteDiscovery\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest --colors=always",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
